    "name": "UAubenTracker",
    "slug": "uaubentracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.uauben.tracker",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET"
      ]
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}
