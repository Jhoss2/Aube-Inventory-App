{
  "name": "U-Auben Tracker",
  "short_name": "U-Auben",
  "description": "Application de gestion logistique pour l'Universit√© Aube Nouvelle",
  "version": "1.0.0",
  "platform": "android",
  "orientation": "any",
  "icon": "./assets/icon.png",
  "splash": {
    "image": "./assets/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#fde7f3"
  },
  "android": {
    "package": "com.uauben.inventory",
    "versionCode": 1,
    "permissions": [
      "CAMERA",
      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE",
      "ACCESS_NETWORK_STATE"
    ],
    "config": {
      "landscape": true
    }
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "expo-sqlite": "~11.3.3",
    "expo-file-system": "~15.4.4",
    "expo-asset": "~8.10.1",
    "@react-native-community/netinfo": "9.4.1",
    "@expo/vector-icons": "^13.0.0",
    "react-native-webview": "13.2.2",
    "base-64": "^1.0.0",
    "@google/generative-ai": "^0.1.0"
  },
  "build": {
    "codemagic": {
      "workflow": "android-workflow",
      "artifact_type": "apk"
    }
  }
}
